signature a_ y_ ./2 false/0 cons/0 b_ x_ true/0 comm_prefix?/0 nil/0;
comm_prefix? (cons x_ y_) (cons x_ b_) = true;
comm_prefix? (cons x_ y_) (cons a_ b_) = false;
comm_prefix? x_ nil = false;
comm_prefix? nil x_ = false;

# comm_prefix? takes in two lists, and returns true if the lists have any common prefix, and false otherwise.

# Examples
# comm_prefix? (cons 4 nil) (cons 4 (cons 5 nil)) = true
# comm_prefix? nil (cons 6 (cons 7 nil)) = false
