signature ./2 pair_fb/0 nil/0 cons/0 x_ head/0 last/0 y_ y/0;
assume 'library/lists/selectors/head';
assume 'library/lists/selectors/last_pair';
pair_fb nil = nil;
pair_fb (cons x_ nil) = cons x_ (cons x_ nil);
pair_fb x_ = cons (head x_) (last_pair x_);

# pair_fb takes in a list, 'x_', and returns a list consisting of the first and last item of 'x_'.

# Examples
# pair_fb (cons 4 nil) = cons 4 (cons 4 nil)
# pair_fb (cons 5 (cons 6 (cons 7 nil))) = cons 5 (cons 7 nil)
