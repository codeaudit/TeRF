signature ./2 last_pair/0 cons/0 x_ nil/0 y_;
last_pair (cons x_ nil) = cons x_ nil;
last_pair (cons x_ y_) = last_pair y_;
last_pair nil = nil;

# last_pair returns the last pair in a non-empty, finite list.

# Examples
# last_pair nil = nil
# last_ pair (cons 4 nil) = cons 4 nil
# last_pair (cons 4 (cons 5 (cons 6 nil))) = cons 6 nil
