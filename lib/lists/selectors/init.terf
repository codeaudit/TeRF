signature cons/0 init/0 nil/0 init1/0 ./2;
assume 'library/lists/constructors/list_items';
init nil = nil;
init (cons x_ nil) = nil;
init xs_ = init1 xs_ nil;
init1 (cons x_ nil) y_ = list y_;
init1 (cons x_ y_) nil = init1 y_ x_;
init1 (cons x_ y_) z_ = init1 y_ (z_ x_);

# in a non-empty list, init returns all the elements of the list except for the last one. in an empty list, init returns nil (the empty list).

# Examples
# init (cons 4 (cons 5 nil)) = cons 4 nil
# init (cons 4 nil) = nil
